{
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true,
    "modules" : true,
    "arrowFunctions" : true,
    "defaultParams" : true,
    "destructuring" : true,
    "generators" : true,
    "restParams" : true,
    "spread" : true,
    "templateStrings" : true
  },
  "env" : {
    "node" : true,
    "jquery" : true,
    "browser" : true
  },
  "rules": {
    "semi": 2,
    "strict": [2, "global"],
    "no-underscore-dangle": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-fallthrough": 0,
    "new-cap": 0,
    "no-loop-func": 0,
    "no-unreachable": 0,
    "no-process-exit": 0,
    "quotes": [1, "double", "avoid-escape"]
  }
}
